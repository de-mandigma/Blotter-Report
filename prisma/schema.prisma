// filepath: prisma/schema.prisma
datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Blotter {
  id          Int      @id @default(autoincrement())
  type        String
  description String
  address     String
  fullName    String
  idPicture   String
  userId      String
  createdAt   DateTime @default(now())
}